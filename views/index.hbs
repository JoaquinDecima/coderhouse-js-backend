<div class="container" style="margin-top:20px; margin-bottom:20px;">
  <div class="row">


    {{#each productos}}
      <div class="col-md-3">
        <div class="card text-center" style="margin-bottom: 10px;">
          <img
            src="{{this.thumbnail}}"
            class="card-img-top"
            style="height: 300px;"
            alt="{{this.title}}">
          <div class="card-body">
            <h5 class="card-title">{{this.title}}</h5>
            <div class="btn btn-primary">$ {{this.price}}</div>
          </div>
        </div>
      </div>
    {{/each}}



  </div>
</div>
